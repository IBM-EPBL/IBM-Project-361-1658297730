float temp;
void setup()
{
pinMode(4,INPUT);
pinMode(5,OUTPUT);
Serial.begin(9600);
}
 void loop()
[
if(digitalRead(4)==HIGH)
{
tone(5,523,1000);
Serial.println("Unknown Detected");
}
else{
boTone(5);
}
temp=analogRead(A1);
temp=temp*0.48828125;
if(temp>=110.84){
tone(12,100,2000);
Serial.print("Above 60 c Temperature...");
}
else{
noTone(12);
}
